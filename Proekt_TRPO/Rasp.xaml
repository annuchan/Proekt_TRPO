<Window x:Class="Proekt_TRPO.Rasp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proekt_TRPO"
        mc:Ignorable="d"
        Title="Rasp" Height="450" Width="800">
    <Grid>
        <ListView ItemsSource="{Binding Raspisanie}" Padding="0, 0, 200, 0" Background="#eaf4f4">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Время" DisplayMemberBinding="{Binding Time}"/>
                    <GridViewColumn Header="Кабинет" DisplayMemberBinding="{Binding Kab}"/>
                    <GridViewColumn Header="День недели" DisplayMemberBinding="{Binding Day}"/>
                    <GridViewColumn Header="Предмет" DisplayMemberBinding="{Binding Predmet}"/>
                    <GridViewColumn Header="Преподаватель" DisplayMemberBinding="{Binding Prepod}"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
